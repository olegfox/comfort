    {#{% stylesheets#}
    {#'bundles/aslanpage/js/jquery-colorbox/example3/colorbox.css'#}
    {#'bundles/aslanpage/css/animate.min.css'#}
    {#'bundles/aslanpage/css/main.css'#}
    {#'bundles/aslanpage/css/slick-theme.css'#}
    {#'bundles/aslanpage/css/slick.css'#}
    {#'bundles/aslanpage/css/custom.css'#}
    {#%}#}
    {#<link href="{{ asset_url }}" type="text/css" rel="stylesheet" />#}
