    <link rel="stylesheet" href="{{ asset('bundles/aslanpage/js/jquery-colorbox/example3/colorbox.css') }}" type="text/css"/>
    <link rel="stylesheet" href="{{ asset('bundles/aslanpage/css/animate.min.css') }}" type="text/css"/>
    <link rel="stylesheet" href="{{ asset('bundles/aslanpage/css/main.css') }}" type="text/css"/>
    <link rel="stylesheet" href="{{ asset('bundles/aslanpage/css/slick-theme.css') }}" type="text/css"/>
    <link rel="stylesheet" href="{{ asset('bundles/aslanpage/css/slick.css') }}" type="text/css"/>
