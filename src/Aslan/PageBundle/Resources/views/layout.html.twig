    <div class="wrap-line level-block level122">
        <div class="shadow"></div>
        <div class="line">

            <!-- Center Block -->
            <div class="wrap-center-block">
                <div class="center-block">
                    <div class="text-block">
                        <p style="text-align:center"><span style="font-size:20px"><span style="color:#FFFFFF">Так же Вы можете позвонить нам по телефону: +7(965)160-11-66</span></span></p>

                        <p style="text-align:center"><span style="font-size:20px"><span style="color:#FFFFFF">Мы ждем Вашего звонка!&nbsp;</span></span></p>
                    </div>
                </div>
            </div>
            <!-- end Center Block -->

        </div>
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="http://code.jquery.com/jquery-1.11.3.min.js"></script>
    <script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/jquery-colorbox/jquery.colorbox-min.js") }}" id="scripts"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/jquery-validation/dist/jquery.validate.min.js") }}" id="scripts"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/jquery-form/jquery.form.js") }}" id="scripts"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/vide/dist/jquery.vide.js") }}" id="scripts"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/vide/dist/jquery.vide.js") }}" id="scripts"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/wow.min.js") }}" id="scripts"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/main.js") }}" id="scripts"></script>
    <script type="text/javascript" src="{{ asset("bundles/aslanpage/js/slick.min.js") }}" id="scripts"></script>
    <script type="text/javascript">
        $(function(){
            var initSlider = function() {
                var options = {
                    infinite: true,
                    slidesToShow: 3,
                    slidesToScroll: 3
                };
                if($('.comment ul').length > 0) {
                    if ($(window).width() > 768) {
                        $('.comment ul').slick(options);
                    } else {
                        $('.comment ul').slick();
                    }
                }
            }

            initSlider();

            $(window).resize(function() {
                $('.comment ul').slick('unslick');
                initSlider();
            });
        });
    </script>
{% endblock %}

