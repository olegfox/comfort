<a href="{{ path('AslanPageBundle_video') }}" onclick="rout(href); return false;">{{ 'VIDEO' | trans }}</a>

<a href="{{ path('AslanPageBundle_photo') }}" onclick="rout(href); return false;">{{ 'PHOTO' | trans }}</a>



</div>    
    
        
    </td>

