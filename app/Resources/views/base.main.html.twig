<script type="text/javascript" src="{{ asset('js/jquery-1.7.2.min.js') }}"></script>
<script src="http://malsup.github.com/jquery.form.js"></script>
{% block script %}
{% endblock %}
<script src="{{ asset('js/jquery.videobackground.js') }}"></script>
<script>
			$(document).ready(function() {
				$('dl').toggle();
				$('h2').bind('click', function(event) {
					event.preventDefault();
					$(this).next('dl').slideToggle(500, function() {
						$('.video-background').videobackground('resize');
					});
				});
				$('body').prepend('<div class="video-background"></div>');
				$('.video-background').videobackground({
					videoSource: [['{{ asset('video/video.mov') }}', 'video/mp4'], ['{{ asset('video/video.ogg') }}', 'video/ogg'], ['{{ asset('video/video.mp4') }}', 'video/mp4']], 
					controlPosition: '#main',
					autoplay:true,
					resizeTo:"document",
					loop:"true",
					poster: '{{ asset('media/img/bg.gif') }}'
				});
			});
</script>
{% block style %}
{% endblock %}		
{% include 'AslanPageBundle:Default:style.new.css.twig' %}
{% include 'AslanPageBundle:Js:script.js.twig' %}
</head>
